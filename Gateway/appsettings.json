{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ReverseProxy": {
    "Routes": {
      "auth": {
        "ClusterId": "identity",
        "Match": { "Path": "/api/auth/{**catch-all}" }
      },
      "users": {
        "ClusterId": "identity",
        "Match": { "Path": "/api/users/{**catch-all}" }
      },
      "venues": {
        "ClusterId": "venue",
        "Match": { "Path": "/api/venues/{**catch-all}" }
      }
    },
    "Clusters": {
      "identity": { "Destinations": { "d1": { "Address": "http://identity-service/" } } },
      "venue": { "Destinations": { "d1": { "Address": "http://venue-service/" } } }
    }
  },
  "AllowedHosts": "*"
}
